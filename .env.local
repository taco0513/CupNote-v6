# ============================================================================
# Development Configuration
# ============================================================================
APP_ENV=development
APP_VERSION=6.0.0
APP_BUILD_NUMBER=1

# Feature flags for development
FEATURE_ACHIEVEMENTS_ENABLED=true
FEATURE_COMMUNITY_MATCH_ENABLED=true
FEATURE_REAL_TIME_SYNC_ENABLED=true
FEATURE_OFFLINE_MODE_ENABLED=true
FEATURE_DEBUG_MODE_ENABLED=true

# ============================================================================
# Performance Settings
# ============================================================================
CACHE_TTL_SECONDS=1800
MAX_CACHE_SIZE_MB=25
ENABLE_PERSISTENT_CACHE=true
DEFAULT_PAGE_SIZE=10
MAX_PAGE_SIZE=50

# ============================================================================
# Localization
# ============================================================================
DEFAULT_LANGUAGE=ko
DEFAULT_LOCALE=ko-KR
TIMEZONE=Asia/Seoul

# ============================================================================
# Development & Debugging
# ============================================================================
DEBUG_MODE=true
LOG_LEVEL=debug
ENABLE_REDUX_DEVTOOLS=true
ENABLE_FLIPPER=true
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_SAMPLE_RATE=1.0

# ============================================================================
# Security (Development)
# ============================================================================
SESSION_TIMEOUT_HOURS=72
REFRESH_TOKEN_THRESHOLD_MINUTES=15
API_RATE_LIMIT_PER_MINUTE=120
ACHIEVEMENT_CHECK_COOLDOWN_SECONDS=2

# ============================================================================
# Privacy (Development)
# ============================================================================
ANALYTICS_ENABLED=false
CRASH_REPORTING_ENABLED=false
TELEMETRY_ENABLED=false

# ============================================================================
# Database (Development)
# ============================================================================
DB_POOL_SIZE=5
DB_TIMEOUT_SECONDS=15
AUTO_MIGRATE=true
MIGRATION_TIMEOUT_SECONDS=30
ENABLE_AUTO_BACKUP=false

# ============================================================================
# Testing
# ============================================================================
ENABLE_MOCK_SERVICES=false
MOCK_DELAY_MS=200

# ============================================================================
# Development URLs
# ============================================================================
DEV_SERVER_URL=http://localhost:3000
API_BASE_URL=${EXPO_PUBLIC_SUPABASE_URL}/rest/v1
ADMIN_PANEL_URL=${EXPO_PUBLIC_SUPABASE_URL}

# ============================================================================
# Build Settings
# ============================================================================
METRO_MAX_WORKERS=2
METRO_CACHE_ENABLED=true
HERMES_ENABLED=true
PROGUARD_ENABLED=false
FLIPPER_VERSION=0.182.0